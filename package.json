{
  "name": "organiser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/client/vue.config.js vue-cli-service serve",
    "build": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/client/vue.config.js vue-cli-service build",
    "lint": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/client/vue.config.js vue-cli-service lint",
    "server": "nodemon app.js",
    "dev": "NODE_ENV=dev concurrently \"npm run server\" \"npm run client\"",
    "test": "NODE_ENV=test concurrently \"npm run server\" \"npm run client\"",
    "cy": "CYPRESS_NODE_ENV=test cypress open",
    "test:unit": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/client/vue.config.js vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap-icons": "^1.5.0",
    "concurrently": "^6.1.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cypress": "^8.7.0",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.8",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "uuid": "^8.3.2",
    "v-calendar": "^2.3.4",
    "vue": "^2.6.11",
    "vue-meta": "^2.4.0",
    "vue-resource": "^1.5.3",
    "vue-router": "^3.2.0",
    "vue-wait": "^1.5.3",
    "vue2-timepicker": "^1.1.6",
    "vuejs-datepicker": "^1.6.2",
    "vuetify": "^2.5.9",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.2.0",
    "vue-cli-plugin-source-dir": "~0.1.0",
    "vue-cli-plugin-vuetify": "^2.4.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "overrides": [
      {
        "env": {
          "jest": true
        },
        "files": [
          "*/__tests__/.{j,t}s?(x)",
          "*/tests/unit/**/.spec.{j,t}s?(x)"
        ]
      }
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier",
      "plugin:cypress/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
